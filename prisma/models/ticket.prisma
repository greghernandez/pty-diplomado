type Ticket {
    id: ID! @id
    movie: Movie! @relation(link: INLINE)
    MovieShow: MovieShow! @relation(link: INLINE)
    seating: String!
    room: Room! @relation(link: INLINE) 
    user: User! @relation(link: INLINE)
    cost: Float!
    payMethod: PaymentType!
    earnedPoints: Int!
}

enum PaymentType {
    CASH
    DEBIT
    CREDIT
    MEMBERSHIP
}